{
  "name": "@heathprovost/nawsso",
  "version": "1.5.3",
  "description": "Node AWS SSO Credentials Helper",
  "author": "Heath Provost <galvanash@gmail.com>",
  "homepage": "https://github.com/heathprovost/nawsso",
  "repository": "github:heathprovost/nawsso",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "nawsso": "./bin/run"
  },
  "oclif": {
    "bin": "nawsso",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/patches"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-sdk/client-sso": "^3.41.0",
    "@aws-sdk/credential-providers": "^3.42.0",
    "@oclif/command": "^1.8.3",
    "@oclif/config": "^1.17.1",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^3.2.5",
    "await-spawn": "^4.0.2",
    "ini": "^2.0.0",
    "patch-package": "^6.4.7"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/node": "^16.11.9",
    "typescript": "^4.5.2"
  }
}
